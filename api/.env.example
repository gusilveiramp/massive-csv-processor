# API
NODE_PORT=3333
SOCKET_PORT=3334

# PostgreSQL
POSTGRES_USER=postgres
POSTGRES_PASSWORD=docker
POSTGRES_DB=csvprocessor
POSTGRES_PORT=5432
DATABASE_URL="postgresql://postgres:docker@localhost:5432/csvprocessor?schema=public"

# Redis
REDIS_PORT=6379
