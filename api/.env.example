# Csv upload
CSV_CHUNK_SIZE=1000
CSV_PROCESSOR_CONCURRENCY=3
CSV_MAX_FILE_SIZE_MB=30

# Exchange rates
CURRENCY_API_URL="https://cdn.jsdelivr.net/npm/@fawazahmed0/currency-api@latest/v1/currencies"
CURRENCY_BASE=usd
CURRENCY_TARGETS=eur,gbp,jpy,brl,cad
